search_initial([],State) :- initial_marking(State).search_initial([Action|As],InState) :-	  trans(Action,PredState,InState),search_initial(As,PredState).trans(A,pre(A,P),P).trans(A,or(X,_Y),XA) :- trans(A,X,XA).trans(A,or(_X,Y),YA) :- trans(A,Y,YA).trans(A,par(X,Y),par(XA,Y)) :- trans(A,X,XA).trans(A,par(X,Y),par(X,YA)) :- trans(A,Y,YA).initial_marking(par(or(pre(a,stop),pre(b,stop)),pre(c,stop))).